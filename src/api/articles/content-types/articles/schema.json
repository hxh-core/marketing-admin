{
  "kind": "collectionType",
  "collectionName": "article",
  "info": {
    "singularName": "articles",
    "pluralName": "article",
    "displayName": "Статьи",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "_softDeletedAt": {
      "type": "datetime",
      "configurable": false,
      "writable": false,
      "visible": false,
      "private": true
    },
    "_softDeletedById": {
      "type": "integer",
      "configurable": false,
      "writable": false,
      "visible": false,
      "private": true
    },
    "_softDeletedByType": {
      "type": "string",
      "configurable": false,
      "writable": false,
      "visible": false,
      "private": true
    },
    "preview": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "content": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "richtext",
      "required": true
    },
    "type": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true
    },
    "page": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::page.page"
    },
    "author": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::authors.authors",
      "inversedBy": "articles"
    },
    "title": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true
    },
    "readingTime": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true
    },
    "description": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "text",
      "required": true
    }
  }
}
